{
  "name": "@web.worker/create-sapi",
  "version": "0.1.1",
  "description": "用最少的配置把 TypeScript 路由函数部署成 HTTP API",
  "keywords": ["api", "hono", "bun", "scaffold", "router"],
  "homepage": "https://github.com/web-worker/sapi",
  "type": "module",
  "bin": {
    "create-sapi": "./index.mjs"
  },
  "scripts": {
    "build": "bun build core/index.ts --outfile template/lib/server.js --target bun",
    "test": "bun test core/",
    "prepublishOnly": "bun run build"
  },
  "files": [
    "index.mjs",
    "template/"
  ],
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "hono": "^4.12.2",
    "json5": "^2.2.3"
  },
  "devDependencies": {
    "@types/bun": "^1.3.9"
  }
}
